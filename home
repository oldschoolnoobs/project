					<script type="text/javascript">
						$('#document').ready(function() {
							$('#btnPollVote_1').click(function() {
							
								var objVotes = {};
								$("input[name='poll_1']").each(function(index) {
									if($(this).is(':checked')) {
										
										objVotes[$(this).attr('id')] = $(this).val();
										
									}
								});
							
								$("input[name='poll_1']").attr('checked', false);
								
								var jsonVotes = JSON.stringify(objVotes);
								var dialogHTML = "";
								$.post('/cs4demo/polls/vote.php', { pollID: '1', pollOptionID: jsonVotes }, function(data) {
									
									try {
										postData = JSON.parse(data);
										
										if(postData['result'] == "success") {
											dialogHTML = "Thank you for voting!";
																	
											
										}
										else {
											dialogHTML = "Unable to vote due to the following reason:<br><br>"+postData['errors'];
										}
										
										
									}
									catch(err) {
										dialogHTML = "You do not have permission to vote on this poll.";
									}
								
									$('#pollDialog_1').html("<p align='center' class='main'>"+dialogHTML+"</p>");
									
									$('#pollDialog_1').dialog({
										title: 'Poll',
										zIndex: 999999,
										modal: true,
										show: 'scale',
										width: 400,
										resizable: false,
										buttons: {
											'Ok': function() {
												$(this).dialog('close');
											}
										}
									
									});
									
								});
							});
						});
					</script>
					
